<div id="dashboard">
  <div class="row-fluid">
    <div class="span6">
      <h2><a href="/readings">Reading Responses</a></h2>
      <% if env['warden'].user.admin? %>
      <a href="/readings/new" class="ladda-button" data-size="m" data-color="blue">Add to the Reading List</a>
      <% end %>
    </div>

    <div class="span6">
      <h2><a href="/experiences">Experiences</a></h2>
    </div>
  </div>

  <div class="row-fluid">
    <h2>Students</h2>
    <% 100.times do |i|%>
    <div class="student-summary row-fluid">
      <div class="span2"><a href="#">Student <%= i %></a></div>

    </div>
    <% end %>
  </div>
</div>